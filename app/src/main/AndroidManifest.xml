<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          
    package="xingyingyue.com.goexplore"
    >

    <application android:allowBackup="true"
        android:label="@string/app_real_name"
        android:icon="@drawable/app_icon"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".Activity.LoginActvity"
            android:launchMode="singleTask"
            android:label="@string/app_real_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Activity.RegisterActivity"
            android:launchMode="singleTask"
            android:label="@string/register_title">
        </activity>
        <activity android:name=".Activity.MainActivity"
            android:launchMode="singleTask"
            android:label="当前位置"></activity>
        <activity android:name=".Activity.EditContentActivity"
            android:label="编写内容">
        </activity>
        <activity android:name=".Activity.ShareContentListActivity"
            android:label="历史感想发布记录"
            android:launchMode="singleTask">
        </activity>
        <activity android:name=".Activity.ShareContentActivity"
            android:label="内容">
        </activity>
    </application>

</manifest>
